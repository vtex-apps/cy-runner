{
  "name": "test-cypress",
  "version": "1.0.0",
  "description": "Just a Cypress fire test",
  "main": "index.js",
  "scripts": {
    "cy:v": "yarn cypress verify",
    "cy:b": "yarn browserslist --update-db",
    "cy:i": "yarn cypress info",
    "cy:w": "cypress run --browser chrome --quiet",
    "test": "yarn cy:v && yarn cy:b && yarn cy:i && yarn cy:w || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtex-apps/cy-runner.git"
  },
  "keywords": [
    "cypress",
    "e2e",
    "automated testing"
  ],
  "author": "charles.santos@vtex.com.br",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vtex-apps/cy-runner/issues"
  },
  "homepage": "https://github.com/vtex-apps/cy-runner#readme",
  "dependencies": {
    "browserslist": "^4.21.4",
    "cypress": "^9.7.0"
  }
}
